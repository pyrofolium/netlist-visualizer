{
  "name": "netlist-visualizer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf build/*",
    "dev-frontend": "vite --config vite.frontend.config.ts",
    "build:frontend": "vite build --config vite.frontend.config.ts",
    "dev-frontend:single": "vite --config vite.frontend.config.ts --open /single.html",
    "build:worker": "vite build --config vite.worker.config.js",
    "build:backend": "tsup",
    "build": "npm run clean && concurrently \"npm run build:frontend\" \"npm run build:backend\"",
    "dev:backend": "tsx src/backend/server.ts",
    "server": "npm run build && node --experimental-specifier-resolution=node build/server.js",
    "lint": "eslint .",
    "preview-frontend": "vite preview",
    "test": "vitest run"
  },
  "dependencies": {
    "@datastructures-js/priority-queue": "^6.3.3",
    "@datastructures-js/queue": "^4.2.3",
    "@types/cors": "^2.8.19",
    "@types/multer": "^2.0.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "fast-deep-equal": "^3.1.3",
    "heapify": "^0.6.0",
    "json-stable-stringify": "^1.3.0",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1",
    "workerpool": "^9.3.3",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.1.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/workerpool": "^6.4.7",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}
